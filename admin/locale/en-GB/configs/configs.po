msgid "global_meta_maxlaenge_title_desc"
msgstr "Specify the maximum number of characters that a meta title may have (including spaces)."

msgid "global_meta_maxlaenge_description_name"
msgstr "Maximum number of characters in meta description"

msgid "global_meta_maxlaenge_description_desc"
msgstr "Specify the maximum number of characters that a meta description may have (including spaces)."

msgid "news_benutzen_name"
msgstr "Use blog"

msgid "news_benutzen_desc"
msgstr "Specify whether to display the blog in the front end of JTL-Shop."

msgid "news_kommentare_nutzen_name"
